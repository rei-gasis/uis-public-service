   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT flex_value_id project_type_id ,
  flex_value project_type_display
FROM FND_FLEX_VALUES
WHERE flex_value_set_id =
  (SELECT flex_value_set_id
  FROM FND_FLEX_VALUE_SETS
  WHERE flex_value_set_name = 'XXUP_PUBLIC_SERVICE_PROJECT_TYPE'
  )
ORDER BY flex_value
   ]]></SQLQuery>

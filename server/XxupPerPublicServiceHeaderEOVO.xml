  XxupPerPublicServiceHeaderEO.ATTRIBUTE5,
  XxupPerPublicServiceHeaderEO.LAST_UPDATE_DATE,
  XxupPerPublicServiceHeaderEO.LAST_UPDATED_BY,
  XxupPerPublicServiceHeaderEO.LAST_UPDATE_LOGIN,
  XxupPerPublicServiceHeaderEO.CREATED_BY,
  XxupPerPublicServiceHeaderEO.CREATION_DATE,
  pp.name POSITION_NAME,
  toa.activity_type ACTIVITY_TYPE_DISPLAY
FROM XXUP.XXUP_PER_PUBLIC_SERVICE_HEADER XxupPerPublicServiceHeaderEO,
  per_positions pp,
  (SELECT flex_value_id activity_id ,
    flex_value activity_type
  FROM FND_FLEX_VALUES
  WHERE flex_value_set_id =
    (SELECT flex_value_set_id
    FROM FND_FLEX_VALUE_SETS
    WHERE flex_value_set_name = 'XXUP_PUBLIC_SERVICE_ACTIVITY_TYPES'
    )
  ORDER BY flex_value
  ) toa
WHERE pp.position_id                = XxupPerPublicServiceHeaderEO.POSITION_ID
AND toa.activity_id (+)             = XxupPerPublicServiceHeaderEO.TYPE_OF_ACTIVITY
   ]]></SQLQuery>

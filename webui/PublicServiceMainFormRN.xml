                              <oa:messageCheckBox id="Selected" dataType="VARCHAR2" text="Selected" prompt="Selected" viewName="XxupPerPublicServiceSubjEOVO1" viewAttr="Selected" defaultValue="" checkedValue="Y" unCheckedValue="N"/>

                  <oa:messageTextInput id="StartDate" dataType="DATE" prompt="* Start Date" viewName="XxupPerPublicServiceHeaderEOVO1" viewAttr="StartDate" required="no"/>
                  <oa:messageTextInput id="EndDate" dataType="DATE" prompt="End Date" viewName="XxupPerPublicServiceHeaderEOVO1" viewAttr="EndDate"/>
                  <oa:messageTextInput id="DurationHours" dataType="NUMBER" prompt="No. of Hours (if less than 1 day)" viewName="XxupPerPublicServiceHeaderEOVO1" viewAttr="DurationHours"/>

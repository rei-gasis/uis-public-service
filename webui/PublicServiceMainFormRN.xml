                  <oa:messageLovInput id="Country" dataType="VARCHAR2" prompt="* Country" viewName="XxupPerPublicServiceHeaderEOVO1" viewAttr="Country" maximumLength="100" queryable="true" externalListOfValues="/xxup/oracle/apps/per/publicservice/lov/webui/PerPSCountryRN" showRequired="false">
                     <lovMappings>
                        <lovMap id="lovMap8" lovItem="Country" resultTo="Country" criteriaFrom="Country"/>
                     </lovMappings>
                  </oa:messageLovInput>
                  <oa:messageLayout id="AddressML" rendered="${oa.XxupPerPublicServiceHeaderEOVO1.RenderAddress}">
                     <ui:contents>
                        <oa:advancedTable id="AddressAT" extends="/xxup/oracle/apps/per/publicservice/webui/PublicServiceAddressRN"/>
                     </ui:contents>

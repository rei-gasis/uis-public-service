               <ui:contents>
                  <oa:messageStyledText id="SequenceNo" dataType="NUMBER" prompt="Sequence No" viewName="XxupPerPublicServiceHeaderEOVO1" viewAttr="SequenceNo" styleClass="OraDataText"/>
